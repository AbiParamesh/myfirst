    "def passwordCheck():\n",
    "    chance = 1\n",
    "    while(chance <= 3):\n",
    "        password = input(\"Enter a strong password: \")\n",
    "        if password == \"stop\":\n",
    "            return\n",
    "        letters = set(password)\n",
    "        special = ['$','@','&','*','_','%','#','>','<', '[', ']','(',')','!', '$','^', '{','}','+','-']\n",
    "        my_list = list(password)\n",
    "        for i in range(len(password)):\n",
    "            if password[i] in special:\n",
    "                a = True\n",
    "        lower = any(letter.islower() for letter in letters)\n",
    "        upper = any(letter.isupper() for letter in letters)\n",
    "        digit = any(letter.isdigit() for letter in letters)\n",
    "        \n",
    "        if len(password) >= 8 and lower is True and upper is True and digit is True and a is True:\n",
    "            print (\"You have entered a strong password\")\n",
    "            return password\n",
    "        else:\n",
    "            print (\"Oops! Password is weak, re-try as per guidelines\")\n",
    "            chance = chance + 1\n",
    "        if chance > 3:\n",
    "            print(\"You have exceeded the attempt limit!\")\n",
    "            return 'Failed'"
   ]
