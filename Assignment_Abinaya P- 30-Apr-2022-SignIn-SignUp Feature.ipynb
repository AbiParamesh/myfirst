    "def signIn():\n",
    "    print (\"****Welcome to Sign In****\")\n",
    "    while(1):      \n",
    "        userNameIn = input (\"Enter your UserName: \").lower()\n",
    "        if userNameIn == \"stop\":\n",
    "            return \"Failed\"\n",
    "        if userNameIn in name_lst:\n",
    "            userNameIn_index = name_lst.index(userNameIn)\n",
    "            chance = 1\n",
    "            while(chance <= 3):\n",
    "                pwIn = input (\"Enter your password: \")\n",
    "                if pass_lst[userNameIn_index] == pwIn:\n",
    "                    print (\"You are now signed in...\")\n",
    "                    return 'Successful'\n",
    "                else: \n",
    "                    print (\"Oops! Incorrect password.. Please try again...\")\n",
    "                    chance = chance + 1\n",
    "                if chance > 3:\n",
    "                    print(\"You have exceeded the attempt limit!\")\n",
    "                    return 'Failed'\n",
    "        else:\n",
    "            print (\"Oops! Incorrect Username...Please try again...Write stop to exit...\")   "
